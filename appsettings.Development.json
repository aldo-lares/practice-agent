{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "AzureOpenAi": {
    "ApiKey": "YOUR_AZURE_OPENAI_API_KEY",
    "Endpoint": "YOUR_AZURE_OPENAI_ENDPOINT",
    "DeploymentName": "YOUR_DEPLOYMENT_NAME"
  },
  "Practibot":{
    "AgentPath":"PATH_TO_AGENT_DIRECTORY",
    "AgentInit":"PATH_TO_INIT_FILE",
    "AgentInstructions":"PATH_TO_INSTRUCTIONS_FILE",
    "SemanticPlugins": "PATH_TO_SEMANTIC_PLUGINS"
  },
  "CosmosDb": {
    "Endpoint": "YOUR_COSMOS_DB_ENDPOINT",
    "Key": "YOUR_COSMOS_DB_KEY",
    "DatabaseId": "YOUR_DATABASE_ID",
    "ContainerId": "YOUR_CONTAINER_ID"
  },
  "OpenAI": {
    "ApiKey": "YOUR_OPENAI_API_KEY",
    "TranscriptionEndpoint": "YOUR_TRANSCRIPTION_ENDPOINT",
    "DefaultLanguage": "YOUR_DEFAULT_LANGUAGE"
  },
  "Debug":{
    "SaveAudioFiles": true
  }
}
